<h1>Members</h1>

<p class="btn--wrapper">
  <a class="btn btn--action" href="/members/new">Add member</a>
</p>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th>Age</th>
    <th></th>
    <th>Address</th>
    <th></th>
    <th>Email</th>
    <th></th>
  </tr>
  <% for member in @members %>
  <tr>
    <td><a href="/members/<%= member.id %>"><%= member.full_name %></a></td>
    <td>|</td>
    <td><%= member.age %></td>
    <td>|</td>
    <td><%= member.address %></td>
    <td>|</td>
    <td><%= member.email %></td>
    <td>|</td>
  </tr>
  <% end %>
</table>
